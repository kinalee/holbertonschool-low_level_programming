#include <stdio.h>

int main()
{
	char charSize;
	int intSize;
	long longSize;
	long long longLongSize;
	float floatSize;


	printf("Size of a char: % byte(s)", sizeof(charSize));
	printf("Size of an int: % byte(s)", sizeof(intSize));
	printf("Size of a long int: % byte(s)", sizeof(longSize));
	printf("Size of a long long int: % byte(s)", sizeof(longLongSize));
	printf("Size of a float: % % byte(s)", sizeof(floatSize));

	return 0;

}
